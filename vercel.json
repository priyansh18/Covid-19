{
  "version": 2,
  "builds": [
    {
      "src": "template/**/*.html",
      "use": "@vercel/static",
      "config": {
        "output": "template",
        "cleanUrls": true
      }
    },
    {
      "src": "static/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/template/index.html"
    }
  ],
  "env": {
    "PYTHONPATH": "/var/task"
  }
}