{
  "version": 2,
  "builds": [
    {
      "src": "template/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "static/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/(.*)", "dest": "/template/$1" },
    { "src": "/", "dest": "/template/index.html" }
  ]
}